html
  head
    link(href='/css/upload.css', rel='stylesheet')
    link(href='/css/menu.css', rel='stylesheet')
    link(rel='shortcut icon', href='/images/GWhite.png')
    title Upload
  body
    script(src='//code.jquery.com/jquery-1.11.2.min.js')
    script(src='//code.jquery.com/jquery-migrate-1.2.1.min.js')
    a(href='/logout')
      p.logout Logout
    a(href='/dashboard')
      img.logo(src='/images/GossetMoustacheClear.png')
    .menu
      .menuContent
        ul#tabs
          <!--a(href='/store')
            li Store
          a(href='/cart')
            li Cart[#{cart.length}]-->
    h1.title Upload
    div(class="uploadbox")
      form.upload(class="upload", method='post', action='/upload', enctype='multipart/form-data')
        div(class="file")
          input#file(type='file', name='file', class="inputFile")
          label(for="file") Choose a File...
        input(class="t", type='text', name='title', placeholder='Title', data-multiple-caption="{count} files selected" multiple)
        textarea(class="d", name='description', placeholder='Description', cols="40", rows="50")
        button#button(type='submit') Submit
    script(src='/scripts/upload.js')
